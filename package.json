{
  "name": "lit-elements-demo",
  "version": "0.1.0",
  "dependencies": {
    "lit-element": "^2.2.0"
  },
  "devDependencies": {
    "typescript": "^3.5.3",
    "http-server": "^0.11.1",
    "husky": "^3.0.0",
